generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(cuid())
  email    String @unique
  password String
}

model Contact {
  id          String @id @default(cuid())
  name        String
  phoneNumber String @unique @map("phone_number")
}
